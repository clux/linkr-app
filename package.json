{
  "name": "linkr-app",
  "description": "Experiments with koa",
  "author": "Eirik Albrigtsen <analsandblaster@gmail.com>",
  "private": true,
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "clux/linkr-app"
  },
  "main": "server",
  "scripts": {
    "init": "make assets && ./init_db.js",
    "start": "node app.js",
    "test": "LINKR_SILENT=1 bndg test/*.test.js",
    "testverbose": "bndg test/*.test.js",
    "precoverage": "istanbul instrument server -o server-cov",
    "coverage": "istanbul cover bndg test/*.test.js > /dev/null && cat coverage/lcov.info",
    "coveralls": "npm run coverage && cat coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "co-bcrypt": "^1.0.0",
    "co-body": "^2.0.0",
    "co-views": "^1.0.0",
    "koa": "^0.21.0",
    "koa-jwt": "^1.1.0",
    "koa-logger": "^1.2.2",
    "koa-mount": "^1.3.0",
    "koa-route": "^2.4.1",
    "koa-static": "^1.4.9",
    "pg": "^4.4.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.2.0",
    "swig": "^1.4.2"
  },
  "devDependencies": {
    "bandage": "^0.2.1",
    "co": "^4.5.4",
    "co-fs": "^1.2.0",
    "co-request": "^0.2.1",
    "coveralls": "^2.11.2",
    "istanbul-harmony": "^0.3.12",
    "vulcanize": "^1.8.1"
  }
}
