{
  "name": "linkr-app",
  "author": "Eirik Albrigtsen <analsandblaster@gmail.com>",
  "description": "Link sharing express server with web components",
  "private": true,
  "repository": {
    "type": "git",
    "url": "clux/linkr-app"
  },
  "version": "0.0.0",
  "scripts": {
    "build-wc": "./build.sh",
    "build-db": "./init_db.js",
    "build": "npm run build-wc && npm run build-db",
    "start": "node app.js",
    "test": "nodeunit test/*.test.js --reporter=verbose",
    "coverage": "jscoverage server && LINKR_COV=1 nodeunit --reporter=lcov test/*.test.js",
    "coveralls": "npm run coverage | coveralls"
  },
  "main": "server",
  "dependencies": {
    "bcrypt": "^0.8.3",
    "body-parser": "^1.12.0",
    "cookie-parser": "^1.3.4",
    "ejs": "^2.3.1",
    "ejs-locals": "^1.0.2",
    "express": "^4.12.2",
    "express-session": "^1.10.3",
    "helmet": "^0.9.1",
    "morgan": "^1.5.1",
    "passport": "^0.2.1",
    "passport-local": "^1.0.0",
    "pg": "^4.3.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.1.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "jscoverage": "^0.5.9",
    "nodeunit": "^0.9.1",
    "request": "^2.53.0",
    "vulcanize": "^1.8.1"
  }
}
